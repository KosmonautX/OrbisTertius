          info: stringify_richtext.(prop["Info"]["rich_text"]),
          geolocation: %{
            live: %{
              latlon: %{
                lat:
                  stringify_richtext.(prop["Coordinate"]["rich_text"])
                  |> String.split(",")
                  |> Enum.map(&String.trim/1)
                  |> Enum.map(&String.to_float/1)
                  |> Enum.at(0),
                lon:
                  stringify_richtext.(prop["Coordinate"]["rich_text"])
                  |> String.split(",")
                  |> Enum.map(&String.trim/1)
                  |> Enum.map(&String.to_float/1)
                  |> Enum.at(1)
              },
              target:
                stringify_richtext.(prop["Radius"]["rich_text"])
                |> String.trim()
                |> String.to_integer(),
              geolock: true
            }
          },
          orb_nature: "01",
          media: media,
          traits: ["pin", "treasurehunt"]
        }

      _ ->
        nil
    end
  end
